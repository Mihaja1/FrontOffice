<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
            <from>^/products/(.*)$</from>
            <to>/product-details/$1</to>
    </rule>
    <rule>
            <from>^/list/(.*)$</from>
            <to>/logins/$1</to>

    </rule>
    <rule>
            <from>^/back/(.*)$</from>
            <to>/login/$1</to>
    </rule>
</urlrewrite>
